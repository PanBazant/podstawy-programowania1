
import java.util.*;
import java.lang.*;

class Rextester
{  
    public static void main(String args[])
    {   String data = "GATGGAACTTGACTACGTAAATT";
        String res="";
        int A=0, C=0, G=0, T=0;
        for(int i=0;i<data.length();i++){
            if(data.substring(i,i+1).equals("T"))
                res+= "U";
            else 
                res += data.substring(i,i+1);
            
    }
            System.out.printf(res);
         
    }}
